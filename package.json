{
  "name": "nls",
  "version": "0.0.1",
  "scripts": {
    "lint": "npx eslint --ext .ts src/",
    "start": "node -r module-alias/register ./dist --env=production",
    "dev": "dotenv -e .env.local -- nodemon",
    "migrate:local": "dotenv -e .env.local -- npx prisma migrate dev",
    "reset:local": "dotenv -e .env.local -- npx prisma migrate reset",
    "create-user:local": "dotenv -e .env.local -- npx ts-node src/scripts/create-user.ts",
    "migrate:prod": "dotenv -e .env.prod -- npx prisma migrate deploy",
    "reset:prod": "dotenv -e .env.prod -- npx prisma migrate reset",
    "create-user:prod": "dotenv -e .env.prod -- npx ts-node src/scripts/create-user.ts",
    "release": "npx sls deploy"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "exec": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@src": "dist"
  },
  "engines": {
    "node": ">=21.5.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.386.0",
    "@aws-sdk/lib-storage": "^3.367.0",
    "@aws-sdk/s3-request-presigner": "^3.386.0",
    "@prisma/client": "^4.16.2",
    "@quixo3/prisma-session-store": "^3.1.13",
    "@types/express-session": "^1.17.7",
    "@types/multer": "^1.4.7",
    "@types/passport": "^1.0.12",
    "@types/passport-local": "^1.0.35",
    "@types/uuid": "^9.0.2",
    "@vendia/serverless-express": "^4.10.4",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csv": "^6.3.1",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.2.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.3",
    "express-ws": "^5.0.2",
    "fs": "^0.0.1-security",
    "inserturlparams": "^1.0.1",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "ts-command-line-args": "^2.5.1",
    "ts-dotenv": "^0.9.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^11.0.1",
    "@types/jasmine": "^4.3.5",
    "@types/jsonfile": "^6.1.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.202",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.3.3",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "esbuild": "^0.17.19",
    "eslint": "^8.44.0",
    "eslint-plugin-node": "^11.1.0",
    "find": "^0.3.0",
    "fs-extra": "^11.1.1",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
